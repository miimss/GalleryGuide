{
  "expo": {
    "name": "galleryguide",
    "slug": "galleryguide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Gallery Guide helps you to find museums and art galleries in the Helsinki area. Registered users can also save their favorite places, so that they can find them easily again!",
    "githubUrl": "https://github.com/miimss/GalleryGuide/"
  }
}
